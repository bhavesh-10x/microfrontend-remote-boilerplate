{
  "name": "microfrontend-remote-boilerplate",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@bit/xto10x.common.index": "^2.1.73",
    "@xto10x/core": "~0.21.2",
    "@xto10x/storage": "~0.21.0",
    "connected-react-router": "^6.9.1",
    "fs": "^0.0.1-security",
    "history": "^4.10.1",
    "mobile-detect": "^1.4.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "start": "export DEBUG=true && webpack serve --config webpack.dev.js",
    "start:prod": "export DEBUG=false && webpack serve --config webpack.dev.js",
    "build": "export DEBUG=false && export NODE_ENV=production && webpack --config webpack.dev.js",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:fix": "yarn lint:js . --fix && prettier --write .",
    "lint:js:fix": "yarn lint:js . --fix",
    "lint:css": "stylelint './src/**/*.js'",
    "prettier": "prettier --write \"**/*.{js,json,md,yml}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,css,json,md,yml}": [
      "yarn prettier"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.3.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.2.5",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "prettier": "^2.4.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "url-loader": "^4.1.1",
    "util": "^0.12.4",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.8.0"
  }
}
